{
    "api_url": {
        "description": "【通用】API的URL地址",
        "type": "string",
        "hint": "默认为 https://api.siliconflow.cn/v1/images/generations",
        "default": "https://api.siliconflow.cn/v1/images/generations"
    },
    "model_list": {
        "description": "【核心】模型列表配置",
        "type": "list",
        "hint": "在此处配置所有可用的模型。支持自定义模型ID。如果填写了专用Key，该模型将优先使用专用Key，否则使用通用Key池。",
        "items": {
            "type": "object",
            "title": "模型配置",
            "properties": {
                "id": {
                    "type": "string",
                    "title": "模型ID",
                    "description": "API调用的实际模型名称 (如 Qwen/Qwen-Image-Edit-2509, Kwai-Kolors/Kolors)"
                },
                "key": {
                    "type": "string",
                    "title": "专用Key (可选)",
                    "description": "留空则使用全局Key池"
                }
            }
        },
        "default": [
            {"id": "Qwen/Qwen-Image-Edit-2509", "key": ""},
            {"id": "Kwai-Kolors/Kolors", "key": ""},
            {"id": "stabilityai/stable-diffusion-3-medium", "key": ""},
            {"id": "stabilityai/stable-diffusion-xl-base-1.0", "key": ""}
        ]
    },
    "model": {
        "description": "【通用】默认模型名称",
        "type": "string",
        "hint": "当前默认使用的模型ID（需在模型列表中存在）。可通过 #切换模型 指令修改。",
        "default": "Qwen/Qwen-Image-Edit-2509"
    },
    "api_keys": {
        "description": "【通用】API Key 池",
        "type": "list",
        "hint": "全局Key轮询池。当模型未配置专用Key时使用此处。",
        "items": {
            "type": "string",
            "description": "sk-..."
        },
        "default": []
    },
    "prefix": {
        "description": "【基础】指令前缀限制",
        "type": "bool",
        "hint": "开启后，指令需要前缀（如 '#'）或 @机器人 才能触发。",
        "default": true
    },
    "extra_prefix": {
        "description": "【基础】自定义提示词前缀",
        "hint": "用于触发自定义prompt，例如设置为 'bnn'，则使用 'bnn <你的prompt>' 来调用",
        "type": "string",
        "default": "bnn"
    },
    "use_proxy": {
        "description": "【网络】是否启用代理",
        "type": "bool",
        "hint": "如果服务器无法连接API，请开启此项。",
        "default": false
    },
    "proxy_url": {
        "description": "【网络】代理地址",
        "type": "string",
        "hint": "格式: http://127.0.0.1:7890",
        "default": ""
    },
    "user_whitelist": {
        "description": "【权限】用户白名单",
        "type": "list",
        "hint": "只允许列表内的用户使用（管理员除外）。",
        "items": {
            "type": "string",
            "description": "用户QQ号"
        },
        "default": []
    },
    "user_blacklist": {
        "description": "【权限】用户黑名单",
        "type": "list",
        "hint": "禁止列表内的用户使用。",
        "items": {
            "type": "string",
            "description": "用户QQ号"
        },
        "default": []
    },
    "group_whitelist": {
        "description": "【权限】群聊白名单",
        "type": "list",
        "hint": "在白名单内的群聊将不限制使用次数且允许使用。",
        "items": {
            "type": "string",
            "description": "群号"
        },
        "default": []
    },
    "group_blacklist": {
        "description": "【权限】群聊黑名单",
        "type": "list",
        "hint": "禁止列表内的群聊使用。",
        "items": {
            "type": "string",
            "description": "群号"
        },
        "default": []
    },
    "enable_user_limit": {
        "description": "【次数】启用用户限制",
        "type": "bool",
        "hint": "开启后，限制每个用户的调用次数。",
        "default": true
    },
    "enable_group_limit": {
        "description": "【次数】启用群组限制",
        "type": "bool",
        "hint": "开启后，限制每个群组的总调用次数。",
        "default": false
    },
    "enable_checkin": {
        "description": "【签到】启用每日签到",
        "type": "bool",
        "hint": "开启后用户可通过 #手办化签到 获取次数。",
        "default": false
    },
    "checkin_fixed_reward": {
        "description": "【签到】固定奖励",
        "type": "int",
        "hint": "关闭随机奖励时，每次签到获得的次数。",
        "default": 3
    },
    "enable_random_checkin": {
        "description": "【签到】启用随机奖励",
        "type": "bool",
        "default": false
    },
    "checkin_random_reward_max": {
        "description": "【签到】随机奖励最大值",
        "type": "int",
        "default": 5
    },
    "prompt_list": {
        "description": "提示词预设列表",
        "hint": "格式: 触发词:提示词。建议使用指令 #lm添加 管理。",
        "type": "list",
        "default": [
        ]
    }
}
